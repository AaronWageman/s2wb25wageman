extends layout

block content
  h1= title
  -var image_names = ['cheeseburger', 'taco', 'pizza', 'chickenwings', 'chickensandwich']
  -for(let i = 0; i < 3; i++){
    -var number = Math.floor(Math.random() * 5);
    img(src=`/images/${image_names[number]}.jpg` alt=`${image_names[number]}` height='300' width='300')
  -}

